{
  "name": "media-tools",
  "displayName": "Media Processing Tools",
  "description": "Advanced media processing including format conversion, upscaling, and analysis",
  "version": "1.0.0",
  "author": "WhatsApp Bot System",
  "enabled": true,
  "ownerOnly": true,
  "priority": 40,
  "dependencies": [],
