{
  "name": "anti-delete",
  "displayName": "Anti-Delete Recovery",
  "description": "Detects and recovers deleted messages from archived data",
  "version": "1.0.0",
  "author": "WhatsApp Bot System",
  "enabled": true,
  "ownerOnly": true,
  "priority": 30,
  "dependencies": [],
  "commands": [
    {
      "name": "recover",
      "description": "Recover deleted messages from archive",
      "usage": "recover [timeframe]",
      "ownerOnly": true
    },
    {
      "name": "deleted",
      "description": "Show recently deleted messages",
      "usage": "deleted [count]",
      "ownerOnly": true
    },
    {
